    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_gallery2"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:layout_marginStart="48dp"
        android:backgroundTint="@color/white"

        android:elevation="5dp"
        app:icon="@drawable/ic_photo_library_black_24dp"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconSize="50dp"
        app:iconTint="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btn_takepicture"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.96"
        app:rippleColor="@color/btnClick"
        app:shapeAppearance="@style/ShapeAppearance.MaterialComponents.MediumComponent"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp">

    </com.google.android.material.button.MaterialButton>

    <ImageView
        android:id="@+id/iv_input"
        android:layout_width="320dp"
        android:layout_height="235dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="436dp"
        android:src="@drawable/ic_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/original" />

    <ImageView
        android:id="@+id/iv_output"
        android:layout_width="320dp"
        android:layout_height="235dp"

        android:layout_marginStart="16dp"
        android:layout_marginBottom="124dp"
        android:src="@drawable/ic_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/original"
        tools:visibility="invisible" />

    <ImageView
        android:id="@+id/view_org"
        android:layout_width="320dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_input"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_input" />

    <ImageView
        android:id="@+id/view_rsl"
        android:layout_width="320dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_output"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_output" />

    <View
        android:id="@+id/view"
        android:layout_width="94dp"
        android:layout_height="90dp"
        android:foreground="@drawable/group_5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_convert"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/white"
        android:elevation="5dp"

        app:icon="@drawable/ic_change_circle_black_24dp"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconSize="50dp"
        app:iconTint="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.534"
        app:rippleColor="@color/btnClick"
        app:shapeAppearance="@style/ShapeAppearance.MaterialComponents.MediumComponent"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp">

    </com.google.android.material.button.MaterialButton>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_save"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/white"
        android:elevation="5dp"

        app:icon="@drawable/ic_save_alt_black_24dp"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconSize="50dp"
        app:iconTint="@android:color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.884"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.96"
        app:rippleColor="@color/btnClick"
        app:shapeAppearance="@style/ShapeAppearance.MaterialComponents.MediumComponent"
        app:strokeColor="@color/black"
        app:strokeWidth="3dp">

    </com.google.android.material.button.MaterialButton>


</androidx.constraintlayout.widget.ConstraintLayout>